<h1>Listing subcategorias</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Posicion</th>
      <th>Descripcion</th>
      <th>Categoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subcategorias.each do |subcategoria| %>
      <tr>
        <td><%= subcategoria.titulo %></td>
        <td><%= subcategoria.posicion %></td>
        <td><%= subcategoria.descripcion %></td>
        <td><%= subcategoria.categoria %></td>
        <td><%= link_to 'Show', subcategoria %></td>
        <td><%= link_to 'Edit', edit_subcategoria_path(subcategoria) %></td>
        <td><%= link_to 'Destroy', subcategoria, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subcategoria', new_subcategoria_path %>
