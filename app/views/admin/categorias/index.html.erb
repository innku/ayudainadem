<h1>Listing categorias</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Slug</th>
      <th>Prioridad</th>
      <th>Visible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categorias.each do |categoria| %>
      <tr>
        <td><%= categoria.titulo %></td>
        <td><%= categoria.slug %></td>
        <td><%= categoria.prioridad %></td>
        <td><%= categoria.visible %></td>
        <td><%= link_to 'Show', admin_categoria_path(categoria) %></td>
        <td><%= link_to 'Edit', edit_admin_categoria_path(categoria) %></td>
        <td><%= link_to 'Destroy', categoria, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categoria', new_admin_categoria_path %>
